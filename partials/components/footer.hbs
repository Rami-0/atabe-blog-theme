    <style>
        .atabe-footer {
            background-color: #0B66FF;
            color: white;
            margin-top: 64px;
            padding: 40px 16px;
        }

        .atabe-footer-inner {
            max-width: 1320px;
            margin: 0 auto;
        }

        .atabe-footer-logo-section {
            display: flex;
            flex-direction: column;
            align-items: flex-start;
            gap: 8px;
        }

        .atabe-logo {
            width: 112px;
            height: auto;
            color: white;
            display: block;
        }

        .atabe-footer-social {
            display: flex;
            align-items: center;
            gap: 24px;
        }

        .atabe-social-link {
            color: white;
            opacity: 0.9;
            transition: opacity 0.2s;
        }

        .atabe-social-link:hover {
            opacity: 1;
        }

        .atabe-footer-divider {
            border-color: rgba(255, 255, 255, 0.5);
            margin: 24px 0;
        }

        .atabe-footer-title {
            font-size: 24px;
            font-weight: 800;
            letter-spacing: -0.025em;
            margin-bottom: 16px;
            text-align: start;
        }

        .atabe-footer-links-grid {
            display: grid;
            grid-template-columns: repeat(2, 1fr);
            gap: 16px 24px;
        }

        .atabe-footer-links-column {
            display: flex;
            flex-direction: column;
            gap: 8px;
        }

        .atabe-footer-link {
            font-size: 14px;
            line-height: 1.6;
            opacity: 0.95;
            transition: opacity 0.2s;
            text-decoration: none;
            color: white;
        }

        .atabe-footer-link:hover {
            opacity: 1;
            text-decoration: underline;
        }

        .atabe-footer-bottom-links {
            display: grid;
            grid-template-columns: repeat(3, 1fr);
            gap: 8px;
            font-size: 14px;
            font-weight: 600;
            text-align: start;
        }

        .atabe-footer-bottom-links nav {
            display: contents;
        }

        .atabe-footer-bottom-links .nav {
            display: grid;
            grid-template-columns: repeat(3, 1fr);
            gap: 8px;
            list-style: none;
            margin: 0;
            padding: 0;
            width: 100%;
        }

        .atabe-footer-bottom-links .nav li {
            margin: 0;
        }

        .atabe-footer-bottom-links .nav a {
            color: white;
            text-decoration: none;
            padding: 4px 0;
            display: block;
        }

        .atabe-footer-bottom-links .nav a:hover {
            text-decoration: underline;
        }

        .atabe-footer-bottom-link {
            color: white;
            text-decoration: none;
            padding: 4px 0;
        }

        .atabe-footer-bottom-link:hover {
            text-decoration: underline;
        }

        .atabe-footer-mobile {
            display: block;
        }

        .atabe-footer-desktop {
            display: none;
        }

        @media (min-width: 1024px) {
            .atabe-footer-mobile {
                display: none;
            }

            .atabe-footer-desktop {
                display: block;
            }

            .atabe-footer {
                padding: 56px 16px;
            }

            .atabe-logo {
                width: 224px;
            }

            .atabe-footer-grid {
                display: grid;
                grid-template-columns: repeat(4, 1fr);
                gap: 24px;
                margin-bottom: 24px;
            }

            .atabe-footer-logo-column {
                grid-row: span 2;
                display: flex;
                flex-direction: column;
                justify-content: flex-end;
                gap: 8px;
                min-height: 260px;
                position: relative;
                top: 48px;
            }

            .atabe-footer-content-column {
                grid-column: 2 / span 3;
            }

            .atabe-footer-content-column .atabe-footer-title {
                font-size: 20px;
                margin-bottom: 16px;
            }

            .atabe-footer-content-column .atabe-footer-links-grid {
                display: grid;
                grid-template-columns: repeat(3, 1fr);
                gap: 24px;
            }

            .atabe-footer-bottom {
                display: grid;
                justify-content: center;
                gap: 24px;
            }


            .atabe-footer-bottom .nav {
                display: flex;
                flex-wrap: wrap;
                gap: 24px;
                list-style: none;
                margin: 0;
                padding: 0;
            }

            .atabe-footer-bottom .nav li {
                margin: 0;
            }

            .atabe-footer-bottom .nav a {
                color: white;
                text-decoration: none;
                font-size: 18px;
                font-weight: 600;
            }

            .atabe-footer-bottom .nav a:hover {
                text-decoration: underline;
            }

            .atabe-footer-bottom-link {
                font-size: 18px;
            }
        }

        [dir="rtl"] .atabe-footer-links-grid,
        [dir="rtl"] .atabe-footer-bottom-links,
        [dir="rtl"] .atabe-footer-title {
            text-align: right;
        }

        [dir="rtl"] .atabe-footer-logo-section {
            align-items: flex-start;
        }
    </style>

    <script>
        (function () {
            const API_URL = 'https://api.atabe.app/api/v1/properties/most-common/searches';
            const isRTL = document.documentElement.dir === 'rtl' || document.documentElement.lang === 'ar';

            function renderLinks(container, searches) {
                if (!searches || searches.length === 0) return;

                const mainCols = [];
                const mainColsData = [];

                // Group searches into arrays of max 5 items
                for (let i = 0; i < Math.min(searches.length, 15); i += 5) {
                    const columnSearches = searches.slice(i, i + 5);
                    const columnItems = columnSearches.map(search => {
                        if (isRTL) {
                            return search.arTitle || search.enTitle || '';
                        } else {
                            return search.enTitle || search.arTitle || '';
                        }
                    });
                    mainCols.push(columnItems);
                    mainColsData.push(columnSearches);
                }

                // Render columns
                mainCols.forEach((column, colIndex) => {
                    const columnDiv = document.createElement('div');
                    columnDiv.className = 'atabe-footer-links-column';

                    column.forEach((item, itemIndex) => {
                        const link = document.createElement('a');
                        const searchItem = mainColsData[colIndex][itemIndex];
                        link.href = searchItem ? 'https://atabe.app/' + searchItem.query : '#';
                        link.className = 'atabe-footer-link';
                        link.textContent = item;
                        columnDiv.appendChild(link);
                    });

                    container.appendChild(columnDiv);
                });
            }

            // Fetch and render for mobile
            const mobileContainer = document.getElementById('atabe-footer-links-mobile');
            const desktopContainer = document.getElementById('atabe-footer-links-desktop');

            if (mobileContainer || desktopContainer) {
                fetch(API_URL)
                    .then(response => response.json())
                    .then(data => {
                        if (mobileContainer) renderLinks(mobileContainer, data);
                        if (desktopContainer) renderLinks(desktopContainer, data);
                    })
                    .catch(error => {
                        console.error('Error fetching footer links:', error);
                    });
            }
        })();
    </script>
